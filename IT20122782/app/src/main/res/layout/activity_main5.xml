<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/tba_vacc_request"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:backgroundTint="#004E63"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:title="@string/tba_vacc_request"
        app:titleTextColor="@color/white" />

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="354dp"
        android:layout_height="315dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#004E63"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.384"
        app:layout_constraintStart_toStartOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="37dp"
                android:layout_height="match_parent"
                android:text="001" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="44dp"
                android:layout_height="56dp"
                android:text="Irushi" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="93dp"
                android:layout_height="match_parent"
                android:text="2021.08.14" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Sinopharm" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView32"
                android:layout_width="wrap_content"
                android:layout_height="47dp"
                android:text="002" />

            <TextView
                android:id="@+id/textView31"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:text="Thamoda" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:text="2021.04.14" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="121dp"
                android:layout_height="match_parent"
                android:text="Sinopharm" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView36"
                android:layout_width="86dp"
                android:layout_height="62dp"
                android:text="003" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="95dp"
                android:layout_height="match_parent"
                android:text="Nethmi" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="79dp"
                android:layout_height="match_parent"
                android:text="2021.08.14" />

            <TextView
                android:id="@+id/textView33"
                android:layout_width="120dp"
                android:layout_height="match_parent"
                android:text="Sinopharm" />
        </TableRow>
    </TableLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="512dp"
        android:layout_height="184dp"
        android:layout_marginBottom="128dp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout"
        app:srcCompat="@drawable/image2"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/txt_para"
        android:layout_width="97dp"
        android:layout_height="104dp"
        android:text="@string/txt_para"
        android:textColor="@color/txt_textcolor1"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.39"
        tools:layout_editor_absoluteX="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>